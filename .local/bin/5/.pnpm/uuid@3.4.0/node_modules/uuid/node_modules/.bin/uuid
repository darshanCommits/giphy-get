#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/run/media/greeed/Darshan/july31/Projects/weather/.local/bin/5/.pnpm/uuid@3.4.0/node_modules/uuid/bin/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/.local/bin/5/.pnpm/uuid@3.4.0/node_modules/uuid/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/.local/bin/5/.pnpm/uuid@3.4.0/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/.local/bin/5/.pnpm/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/.local/bin/5/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/.local/bin/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/.local/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/node_modules:/run/media/greeed/Darshan/july31/Projects/node_modules:/run/media/greeed/Darshan/july31/node_modules:/run/media/greeed/Darshan/node_modules:/run/media/greeed/node_modules:/run/media/node_modules:/run/node_modules:/node_modules:.local/bin/5/.pnpm/node_modules"
else
  export NODE_PATH="/run/media/greeed/Darshan/july31/Projects/weather/.local/bin/5/.pnpm/uuid@3.4.0/node_modules/uuid/bin/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/.local/bin/5/.pnpm/uuid@3.4.0/node_modules/uuid/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/.local/bin/5/.pnpm/uuid@3.4.0/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/.local/bin/5/.pnpm/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/.local/bin/5/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/.local/bin/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/.local/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/node_modules:/run/media/greeed/Darshan/july31/Projects/node_modules:/run/media/greeed/Darshan/july31/node_modules:/run/media/greeed/Darshan/node_modules:/run/media/greeed/node_modules:/run/media/node_modules:/run/node_modules:/node_modules:.local/bin/5/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/uuid" "$@"
else
  exec node  "$basedir/../../bin/uuid" "$@"
fi
