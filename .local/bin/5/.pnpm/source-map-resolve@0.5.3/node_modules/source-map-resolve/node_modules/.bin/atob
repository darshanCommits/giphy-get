#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/run/media/greeed/Darshan/july31/Projects/weather/.local/bin/5/.pnpm/atob@2.1.2/node_modules/atob/bin/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/.local/bin/5/.pnpm/atob@2.1.2/node_modules/atob/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/.local/bin/5/.pnpm/atob@2.1.2/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/.local/bin/5/.pnpm/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/.local/bin/5/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/.local/bin/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/.local/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/node_modules:/run/media/greeed/Darshan/july31/Projects/node_modules:/run/media/greeed/Darshan/july31/node_modules:/run/media/greeed/Darshan/node_modules:/run/media/greeed/node_modules:/run/media/node_modules:/run/node_modules:/node_modules:.local/bin/5/.pnpm/node_modules"
else
  export NODE_PATH="/run/media/greeed/Darshan/july31/Projects/weather/.local/bin/5/.pnpm/atob@2.1.2/node_modules/atob/bin/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/.local/bin/5/.pnpm/atob@2.1.2/node_modules/atob/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/.local/bin/5/.pnpm/atob@2.1.2/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/.local/bin/5/.pnpm/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/.local/bin/5/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/.local/bin/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/.local/node_modules:/run/media/greeed/Darshan/july31/Projects/weather/node_modules:/run/media/greeed/Darshan/july31/Projects/node_modules:/run/media/greeed/Darshan/july31/node_modules:/run/media/greeed/Darshan/node_modules:/run/media/greeed/node_modules:/run/media/node_modules:/run/node_modules:/node_modules:.local/bin/5/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../atob@2.1.2/node_modules/atob/bin/atob.js" "$@"
else
  exec node  "$basedir/../../../../../atob@2.1.2/node_modules/atob/bin/atob.js" "$@"
fi
